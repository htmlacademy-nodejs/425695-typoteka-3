  "devDependencies": {
    "eslint": "7.26.0",
    "eslint-config-htmlacademy": "0.6.0",
    "eslint-plugin-import": "2.25.3",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "sqlite3": "5.0.2",
    "supertest": "6.1.3"
  },
  "dependencies": {
    "axios": "0.21.1",
    "bcrypt": "5.0.1",
    "chalk": "4.1.0",
    "concurrently": "6.4.0",
    "connect-session-sequelize": "7.1.2",
    "cross-env": "7.0.3",
    "csurf": "1.11.0",
    "dayjs": "1.10.7",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "joi": "17.4.2",
    "lodash.truncate": "4.4.2",
    "multer": "1.4.2",
